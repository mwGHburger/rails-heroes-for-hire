<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path, class: "navbar-brand" do %>
    <%= image_tag "https://propertymanagersites.com/images/109.png" %>
  <% end %>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <button class="btn-red" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"  aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span>HIRE OUT A HERO</span>
    </button>
    <% if user_signed_in? %>
    <%= link_to "Home", root_path, class: "nav-link" %>
    <%= link_to "Dashboard", user_path(current_user), class: "nav-link" %>
    <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
    <% else %>
    <%= link_to "Register", new_user_session_path, class: "nav-link" %>
    <%= link_to "Login", new_user_session_path, class: "nav-link" %>
    <% end %>
  </div>
</div>
